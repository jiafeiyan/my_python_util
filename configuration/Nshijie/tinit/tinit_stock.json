{
    "Syncs": [
      {
        "host": "stock_broker",
        "type": "put",
        "target" : "${HOME}/run/tinit/csv",
        "items": ["${SIM_PLATFORM_HOME}/csvfiles/broker/stock/SG01",
                  "${SIM_PLATFORM_HOME}/csvfiles/broker/stock/SG02",
                  "${SIM_PLATFORM_HOME}/csvfiles/broker/stock"]
      },
      {
        "host": "etf_broker",
        "type": "put",
        "target" : "${HOME}/run/sptinit/csv",
        "items": ["${SIM_PLATFORM_HOME}/csvfiles/broker/etf/SG07"]
      },
      {
        "host": "stock_exchange",
        "type": "put",
        "target" : "${HOME}/run/tinit/dump",
        "items": ["${SIM_PLATFORM_HOME}/csvfiles/exchange/0001"]
      },
      {
        "host": "stock_exchange",
        "type": "put",
        "target" : "${HOME}/run/mdreceiver/bin",
        "items": ["${SIM_PLATFORM_HOME}/csvfiles/exchange/0001/md"]
      },
      {
        "host": "stock_robot",
        "type": "put",
        "target" : "${HOME}/xops/csvfiles/exchange/0001/",
        "items": ["${SIM_PLATFORM_HOME}/csvfiles/exchange/0001/order_info.csv"]
      }
    ]
}
